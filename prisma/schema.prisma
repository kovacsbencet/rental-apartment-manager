generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Property {
  id        Int      @id @default(autoincrement())
  city      String   @default("Wien")
  district  String
  street    String
  number    Int
  stairwell Int
  floor     Int
  apartment Int
  status    Status
  rent      Int
  rentStart DateTime @default(now())
  rentEnd   DateTime @default(now())
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())
}

enum Status {
  RENTED
  NOT_RENTED
}